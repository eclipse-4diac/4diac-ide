				<and>
					<reference definitionId="org.eclipse.fordiac.ide.structuredtextfunctioneditor.STFunction.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
