	<FunctionBody>
		<ST><![CDATA[(* this Function extracts the 8 BOOLS from a byte *)
FUNCTION SPLIT_BYTE_INTO_BOOLS

VAR_INPUT
	IN : BYTE;
END_VAR

VAR_OUTPUT
	BIT_00 : BOOL;
	BIT_01 : BOOL;
	BIT_02 : BOOL;
	BIT_03 : BOOL;
	BIT_04 : BOOL;
	BIT_05 : BOOL;
	BIT_06 : BOOL;
	BIT_07 : BOOL;
END_VAR

BIT_00 := IN.%X0;
BIT_01 := IN.%X1;
BIT_02 := IN.%X2;
BIT_03 := IN.%X3;
BIT_04 := IN.%X4;
BIT_05 := IN.%X5;
BIT_06 := IN.%X6;
BIT_07 := IN.%X7;

END_FUNCTION
]]></ST>
