	<FunctionBody>
		<ST><![CDATA[(* this Function extracts the 2 WORD from a dword *)
FUNCTION SPLIT_DWORD_INTO_WORDS

VAR_INPUT
	IN : DWORD;
END_VAR

VAR_OUTPUT
	WORD_00 : WORD;
	WORD_01 : WORD;
END_VAR

WORD_00 := IN.%W0;
WORD_01 := IN.%X1;

END_FUNCTION
]]></ST>
