OUT_09 := DWORD_TO_BYTE(SHR(IN AND DWORD_QUARTER_09, SHIFT_QUARTER_09));
OUT_10 := DWORD_TO_BYTE(SHR(IN AND DWORD_QUARTER_10, SHIFT_QUARTER_10));
