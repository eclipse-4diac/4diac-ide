	OUT_13 : BYTE;    // QUARTER BYTE 13
	OUT_14 : BYTE;    // QUARTER BYTE 14
	OUT_15 : BYTE;    // QUARTER BYTE 15
END_VAR

OUT_00 := DWORD_TO_BYTE(SHR(IN AND DWORD_QUARTER_00, SHIFT_QUARTER_00));
