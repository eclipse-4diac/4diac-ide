	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Initialization">
				<With Var="QI"/>
				<With Var="ID"/>
			</Event>
			<Event Name="REQ" Type="Event" Comment="Send Data, unconfirmed">
