		</OutputVars>
	</InterfaceList>
	<FunctionBody>
		<ST><![CDATA[PACKAGE signalprocessing;

(* Convert a REAL in the Range 0.0 to 100.0 to a WORD Value Range 0-FAFF *)
FUNCTION FIELDBUS_WORD_TO_PERCENT
VAR_INPUT
	QI : BOOL;
	WI : WORD;
END_VAR

VAR_OUTPUT
	QO : BOOL;
	RO : REAL;
	WO : WORD;
END_VAR

QO := QI;
IF (TRUE = QI) THEN
	IF (WI <= FIELDBUS_VALID_SIGNAL_W) THEN
		WO := WI;
		RO := UDINT_TO_REAL(WORD_TO_UDINT(WO)) / UDINT_TO_REAL(WORD_TO_UDINT(FIELDBUS_VALID_SIGNAL_W));
	END_IF;
END_IF;
END_FUNCTION
]]></ST>
	</FunctionBody>
</Function>
